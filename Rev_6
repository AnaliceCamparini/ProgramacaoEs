#include <cstdlib>
#include<iostream>
using namespace std;

/*
 * 
 */
int main(int argc, char** argv) {
    int n1,resul;
    
    cout<<"Informe um numero que voce deseja saber a tabuada: ";
    cin>>n1;
    cout<<endl<<endl;
    cout<<"**********TABUADA"<<n1<<"**********:"<<endl<<endl;
    
    for(int i=0;i<=10;i++){
        resul=n1*i;
       
        cout<<n1<<"X"<<i<<"="<<resul<<endl;
    }
    cout<<endl<<endl;
    return 0;
}

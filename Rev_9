#include <iostream>
#include<cstring>
#include<cstdlib>

using namespace std;

int main()
{
    int vetor1 [10];
    int vetor2[10];
    int mult[10];//neste caso voce soma 1 na soma para que ela armazene o valor de todos os numeros digitados.

    for(int i=0;i<10;i++){
        cout<<"Digite um número: ";
        cin>>vetor1[i];
    }
    cout<<endl<<endl<<endl;
     for(int i=0;i<10;i++){
        cout<<"Digite um número: ";
        cin>>vetor2[i];
    }

    for(int i=0;i<10;i++){
       mult[i]=vetor1[i]*vetor2[i];
    }

     cout<<endl<<endl<<endl;
     for(int i=0;i<10;i++){
     cout<<"-"<<vetor1[i];
     }
     cout<<endl;
     for(int i=0;i<10;i++){
     cout<<"-"<<vetor2[i];
     }
     cout<<endl;
     for(int i=0;i<10i++){
     cout<<"-"<<mult[i];
     }


    return 0;}

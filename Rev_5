
#include <cstdlib>
#include<iostream>

using namespace std;

/*
 * 
 */
int main(int argc, char** argv) {
    int senha;
   
    for(int i=0;i<3;i++){
        cout<<"Informe a senha: ";
        cin>>senha;
        cout<<endl<<endl;
    
        if(senha!=1234){
            cout<<"Senha INCORRETA";
            cout<<endl<<endl;}
    
        else
           if(senha == 1234){
           cout<<"Senha CORRETA!";
           cout<<endl<<endl;
           break;
           }
    
}    

    return 0;
}

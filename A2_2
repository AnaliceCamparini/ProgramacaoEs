#include <cstdlib>
#include<iostream>
#include<cstring>

using namespace std;

/*
 * 
 */
int main(int argc, char** argv) {
    char palavra[20],palavra2[20];
    cout<<"Digite uma palavra: ";
    cin>>palavra;
    cout<<"Digite uma outra palavra: ";
    cin>>palavra2;
    cout<<"Palavra 1: "<<palavra<<endl;
    cout<<"Palavra 2: "<<palavra2<<endl;
    cout<<"Palavra 2 atualizada: "<<strcpy(palavra2,palavra)<<endl; 
    
   
    
    return 0;
}
